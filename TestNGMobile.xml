<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="TestSuite" parallel="none" >

    <test name="iPhone 6/7/8 Plus Chrome mobile test">

        <parameter name="testBox" value="mobile" />
        <parameter name="mobileDevice" value="iPhone 6/7/8 Plus" />
        <parameter name="browser" value="chrome"/>

